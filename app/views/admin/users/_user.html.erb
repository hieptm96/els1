<tr>
  <td class="text-left"><%= link_to user.name, user %></td>
  <td class="text-left"><%= user.email %></td>
  <td class="text-left"><%= user.address %></td>
  <% if current_user.is_admin? %>
    <% if !current_user?(user) %>
      <td class="text-left">
        <%= link_to t("admin.delete"), admin_user_path(user.id), method: :delete,
          data: {confirm: t("admin.confirm_delete")} %>
      </td>
    <% else %>
      <td class="text-left"> <%= t("admin.admin_show") %> </td>
    <% end %>
  <% end %>
  </td>
</tr>
